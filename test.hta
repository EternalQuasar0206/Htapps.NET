<html>
    <head>
        <meta http-equiv="x-ua-compatible" content="ie=edge" />
        <script src="./src/environment/environment-setup.js"></script>
        <script src="./src/libraries/bluebird.min.js"></script>
        <script src="./src/polyfills/polyfills-function.js" type="text/javascript"></script>
        <script src="./src/polyfills/polyfills-document.js" type="text/javascript"></script>
        <script src="./src/polyfills/polyfills-Object.js" type="text/javascript"></script>
        <script src="./src/polyfills/polyfills-array.js" type="text/javascript"></script>
        <script src="./src/adapters/adapters-sleep.js"></script>
        <script src="./src/adapters/adapters-async.js"></script>
        <script src="./src/adapters/adapters-webfetch.js"></script>
    </head>
    <body>
        <script>
        function clicou() {
            fetch('https://jsonplaceholder.typicode.com/posts/', {
                method: "GET"
           })
           .and(function(x) {
                JSON.parse(x).forEach(function(p) {
                    document.getElementById("posts").innerHTML += 
                    "<b>UserID</b> " + p.userId + "<br>" +
                    "<b>ID</b> " + p.id + "<br>" +
                    "<b>Title</b> " + p.title + "<br>" +
                    "<b>Body</b> " + p.body + "<br><br><br>";
                })
           });
        }

        </script>
        <button onclick="clicou()">CLICKME</button>
        <button>asdsadsa</button>
        <div id="posts">

        </div>
    </body>
</html>